#!/bin/sh

# go path
export GOPATH="${HOME}/git"

# update path
export PATH=/usr/local/bin:/usr/local/go/bin:${PATH}:${GOPATH}/bin:/usr/share/bcc/tools:/sbin:$HOME/.rbenv/bin:$HOME/.rbenv/versions/2.5.0/bin:$HOME/.local/bin:/opt/mssql-tools/bin

# update cdpath
export CDPATH=${CDPATH}:$GOPATH/src/github.com:$GOPATH/src/golang.org:$GOPATH/src:$GOPATH/src/github/alexhokl:$HOME/git

# The next line updates PATH for the Google Cloud SDK.
# shellcheck source=/dev/null
if [ -f "${HOME}/google-cloud-sdk/path.bash.inc" ]; then source "${HOME}/google-cloud-sdk/path.bash.inc"; fi

# The next line enables shell command completion for gcloud.
# shellcheck source=/dev/null
if [ -f "${HOME}/google-cloud-sdk/completion.bash.inc" ]; then source "${HOME}/google-cloud-sdk/completion.bash.inc"; fi
